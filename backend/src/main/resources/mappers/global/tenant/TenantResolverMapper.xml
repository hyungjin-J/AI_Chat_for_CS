<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aichatbot.global.tenant.domain.mapper.TenantResolverMapper">

    <select id="findTenantIdByKey" resultType="java.lang.String">
        SELECT id
        FROM tb_tenant
        WHERE tenant_key = #{tenantKey}
          AND status = 'active'
    </select>

</mapper>
